#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv) {
    
    int j;
    
    scanf("%d", &j);
    
    int arreglo[j];
    
    for(int b=0;b<j;b++){
        scanf("%d", &arreglo[b]);
    }
    
    for(int a=0;a<j;a++){
        printf("%d\n", arreglo[a]);
    }

    return (0);
}
